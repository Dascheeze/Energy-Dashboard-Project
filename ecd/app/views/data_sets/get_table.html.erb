<div class="activeButton">power_info_button</div>

<h1>Listing data_sets</h1>

<table id="dataset">
	<caption>
		Hourly breakdown for <%= @assoc_meter.name %>
	</caption>
	<tr><td></td>
		
<% @data_points.each do |d_point| %>
		<th><%= d_point.created_at %></td>
<% end %>
	</tr>
	<tr>
	<th> <%= @data_set.name %> </th>
<% @data_points.each do |d_point| %>
	<td> <%= d_point.amount %> </td>
<% end %>
</table>

<br />

<%= link_to 'New data_set', new_data_set_path %>