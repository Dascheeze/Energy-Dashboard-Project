<h1>New data_set</h1>

<% form_for(@data_set) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :meter_id %><br />
    <%= f.text_field :meter_id %>
  </p>
  <p>
    <%= f.label :point_number %><br />
    <%= f.text_field :point_number %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :units %><br />
    <%= f.text_field :units %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', data_sets_path %>
