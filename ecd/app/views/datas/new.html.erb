<h1>New data</h1>

<% form_for(@data) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :data_set %><br />
    <%= f.text_field :data_set %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', datas_path %>