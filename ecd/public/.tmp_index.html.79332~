<script id="source">

$(function() {
	 $(".navButton").button();
	
	 $("#building-tabs").tabs();
	
    var stack = 0, bars = true, lines = false, steps = false;
    
    function plotWithOptions(divToPlot) {
		var d1 = [];
		for (var i = 0; i <= 10; i += 1)
			d1.push([i, parseInt(Math.random() * 30)]);

		var d2 = [];
		for (var i = 0; i <= 10; i += 1)
			d2.push([i, parseInt(Math.random() * 30)]);

		var d3 = [];
		for (var i = 0; i <= 10; i += 1)
			d3.push([i, parseInt(Math.random() * 30)]);
			
        $.plot($(divToPlot), [ d1, d2, d3 ], {
            series: {
                stack: stack,
                lines: { show: lines, steps: steps },
                bars: { show: bars, barWidth: 0.6 }
            }
        });
    }
	
	$('.tab-button').click(function() {plotWithOptions($("" + $(this).attr("href") + " ").children(".flot-div"));});
	
	plotWithOptions("#bldg-1-flot");
	
    

	
	/*$(".ui-state-default").hover(
		function(){
			$(this)
			.toggleClass("ui-state-default")
			.toggleClass("ui-state-hover");
		},function(){
			$(this)
			.toggleClass("ui-state-default")
			.toggleClass("ui-state-hover");
		}
	).toggleClass("ui-corner-all");*/
	
});

</script>


